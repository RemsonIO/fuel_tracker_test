<div class="container">
  <h1>Managers Dashboard</h1>

  <table class="table">
    <thead>
      <tr>
        <th scope="col">Driver</th>
        <th scope="col">Vehicle</th>
        <th scope="col">Date</th>
        <th scope="col">Odometer reading</th>
        <th scope="col">Refuel location</th>
        <th scope="col">Liters of fuel</th>
      </tr>
    </thead>
    <tbody>
      <% @refuel_infos.each do |rf| %>
        <tr>
          <th scope="col"><%= %></th>
          <th scope="col"><%= rf.vehicle %></th>
          <th scope="col"><%= rf.date.strftime("%b %d %Y %I:%M %P") if rf %></th>
          <th scope="col"><%= rf.odometer_reading %></th>
          <th scope="col"><%= rf.refuel_location %></th>
          <th scope="col"><%= rf.liters_of_fuel %></th>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
